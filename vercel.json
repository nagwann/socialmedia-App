
{
  "version": 2,
  "name": "social-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "buildCommand": "npm install && npm run build -- --configuration production",
  "outputDirectory": "dist/social-app",
"rewrites": [{ "source": "/(.*)", "destination": "/" }]


}